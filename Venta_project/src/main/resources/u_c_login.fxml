<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="10" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pe.edu.upeu.venta.controller.LoginController">
    <padding>
        <Insets bottom="20" left="20" right="20" top="20" />
    </padding>
    <Label text="Ingrese su &quot;USUARIO&quot; Y &quot;CONTRASEÑA&quot;" />
    <TextField fx:id="txtUsuario" maxWidth="175.0" promptText="Usuario" />
    <PasswordField fx:id="pfClave" maxWidth="200.0" promptText="Contraseña" />
    <HBox alignment="CENTER" spacing="10">
        <Button onAction="#entrarLogin" text="Ingresar" />
    </HBox>
</VBox>
